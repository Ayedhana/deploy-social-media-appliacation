{
    "version":2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "headers": [
                {
                  "key": "access-control-allow-origin",
                  "value": "*"
                }
            ]
        }
    ]
    
  }
